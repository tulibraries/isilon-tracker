<div id="<%= dom_id volume %>">

  <h1 class="volume-name"><%= "Volume: #{volume.name}" %></h1>

  <div
    id="tree"
    class="wb-tree isilon-tree"
    data-controller="wunderbaum"
    data-wunderbaum-url-value="<%= file_tree_volume_path(volume.id) %>"
    style="height: 500px;"
  ></div>

</div>
